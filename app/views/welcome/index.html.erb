<%= button_to "Create New User", register_path, method: :get %>
<section class='existing-users'>
    <% if session[:user_id] != nil %>
        <p>Existing Users: </p>
        <% @users.each do |user| %>
            <li><%= link_to user.email, user_path(user.id) %></li>
        <% end %>
    <% end %>
    <br>
    <% if session[:user_id] == nil %>
        <%= link_to "Log In", '/login' %>
    <% else %>
        <%= link_to "Log Out", logout_path %>
    <% end %>
</section>